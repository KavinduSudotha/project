|created_date|date|No|
|due_date|date|No|
|customer_name|int(30)|Yes|NULL
|address|int(40)|Yes|NULL
|employee_id|int(8)|No|
|height|int(6)|No|
|width|int(6)|No|
|length|int(6)|No|
|ratio_chips|int(4)|No|
|ratio_peat|int(4)|No|
|driver_name|int(40)|Yes|NULL
|vehicle_type|varchar(8)|Yes|NULL
|vehicle_number|int(8)|Yes|NULL
|transport_company|varchar(40)|Yes|NULL
